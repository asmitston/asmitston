#include<stdio.h>
int main(){
    int a,b;
    printf("Enter your 1st number:");
    scanf("%d",&a);
    printf("Enter your 2nd number:");
    scanf("%d",&b);

    char c;

    printf("+,-,*,/,%%:");
    scanf(" %c",&c);

    if(c=='+'){
        printf("sum=%d",a+b);
    }
    if(c=='*'){
        printf("mult=%d",a*b);
    }
    if(c=='-'){
        printf("sub=%d",a-b);
    }
    if(c=='/'){
        printf("div=%d",a/b);
    }

    return 0;

}
