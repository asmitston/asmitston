#include<stdio.h>
int main()
{
    int table[10];
    int i, j, n;
    printf("Enter a table: ");
    scanf("%d", &n);
    printf("Multiplication table of %d:\n", n);
    for(i = 0; i < 10; i++) {
        table[i] = n * (i + 1);
    }
    for(j = 0; j < 10; j++) {
        printf("%d x %d = %d\n", n, j + 1, table[j]);
    }
    return 0;
}
